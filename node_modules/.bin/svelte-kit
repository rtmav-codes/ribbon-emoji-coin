#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rtmav/ribbon-emoji-coin/node_modules/.pnpm/@sveltejs+kit@2.15.1_@sveltejs+vite-plugin-svelte@3.1.2_svelte@4.2.19_vite@5.4.11__svelte@4.2.19_vite@5.4.11/node_modules/@sveltejs/kit/node_modules:/Users/rtmav/ribbon-emoji-coin/node_modules/.pnpm/@sveltejs+kit@2.15.1_@sveltejs+vite-plugin-svelte@3.1.2_svelte@4.2.19_vite@5.4.11__svelte@4.2.19_vite@5.4.11/node_modules/@sveltejs/node_modules:/Users/rtmav/ribbon-emoji-coin/node_modules/.pnpm/@sveltejs+kit@2.15.1_@sveltejs+vite-plugin-svelte@3.1.2_svelte@4.2.19_vite@5.4.11__svelte@4.2.19_vite@5.4.11/node_modules:/Users/rtmav/ribbon-emoji-coin/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rtmav/ribbon-emoji-coin/node_modules/.pnpm/@sveltejs+kit@2.15.1_@sveltejs+vite-plugin-svelte@3.1.2_svelte@4.2.19_vite@5.4.11__svelte@4.2.19_vite@5.4.11/node_modules/@sveltejs/kit/node_modules:/Users/rtmav/ribbon-emoji-coin/node_modules/.pnpm/@sveltejs+kit@2.15.1_@sveltejs+vite-plugin-svelte@3.1.2_svelte@4.2.19_vite@5.4.11__svelte@4.2.19_vite@5.4.11/node_modules/@sveltejs/node_modules:/Users/rtmav/ribbon-emoji-coin/node_modules/.pnpm/@sveltejs+kit@2.15.1_@sveltejs+vite-plugin-svelte@3.1.2_svelte@4.2.19_vite@5.4.11__svelte@4.2.19_vite@5.4.11/node_modules:/Users/rtmav/ribbon-emoji-coin/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
else
  exec node  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
fi
